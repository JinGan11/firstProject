<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ucar.combination.dao.AccountHistoryDao">
    <insert id="insert" parameterType="com.ucar.combination.model.AccountHistory">
        insert into
        t_account_history
        (account_id,history_operation_type,create_time,create_emp,staff_name,staff_num,permissions,account_state)
        values
        (#{acc.accountId},#{acc.historyOperationType},#{acc.createTime},#{acc.createEmp},#{acc.staffName},#{acc.staffNum},#{acc.permissions},#{acc.accountState})
    </insert>
</mapper>